{
  "compilerOptions": {
    "types": ["@vitejs/plugin-rsc/types", "vite/client"],
    "module": "ESNext",
    "moduleResolution": "Bundler",
    "target": "ESNext",
    "jsx": "react-jsx",
    "strict": true,
    "skipLibCheck": true,
    "noEmit": true,
    "baseUrl": ".",
    "paths": {
      "framework/server": ["framework/entry.rsc.ts"],
      "~/*": ["src/*"]
    }
  }
}
